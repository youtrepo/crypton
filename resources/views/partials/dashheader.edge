          <a class="dropdown-item d-flex" href="javascript:void(0);"><img src="/images/ca.png" class="flag-width" alt="flag"> <span class="align-self-center">&nbsp;English</span></a>
        </div>
      </li>

      <li class="nav-item dropdown message-dropdown">
        <a href="javascript:void(0);" class="nav-link dropdown-toggle" id="messageDropdown" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
          <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-mail"><path d="M4 4h16c1.1 0 2 .9 2 2v12c0 1.1-.9 2-2 2H4c-1.1 0-2-.9-2-2V6c0-1.1.9-2 2-2z"></path><polyline points="22,6 12,13 2,6"></polyline></svg>
          <span class="badge badge-primary">0</span>
        </a>
        <div class="dropdown-menu p-0 position-absolute p-4" aria-labelledby="messageDropdown">
          <div class="">
      <a><h6 class="dropdown-header">Messages</h6></a>
            <a class="dropdown-item">
              <div class="pt-2">
                @if(chats.length!==0)
                @each(chat in chats)
                <div class="media notification-new">
                  <div class="notification-icon">
                    <div class="icon-svg mr-3">
                      <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-mail"><path d="M4 4h16c1.1 0 2 .9 2 2v12c0 1.1-.9 2-2 2H4c-1.1 0-2-.9-2-2V6c0-1.1.9-2 2-2z"></path><polyline points="22,6 12,13 2,6"></polyline></svg>
                    </div>
                  </div>
                  <div class="media-body">
                    <p class="meta-title mr-3">{{chat.user}}</p>
                    <p class="message-text">{{chat.messages}}</p>
                    <p class="meta-time align-self-center mb-0">{{moment(chat.created_at).format("DD MMMM y")}}</p>
                  </div>
                  @endeach
                  @else
                  <div class="media notification-new">
                    <div class="notification-icon">
                      <div class="icon-svg mr-3">
                        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-frown"><circle cx="12" cy="12" r="10"/><path d="M16 16s-1.5-2-4-2-4 2-4 2"/><line x1="9" y1="9" x2="9.01" y2="9"/><line x1="15" y1="9" x2="15.01" y2="9"/></svg>
                      </div>
                    </div>
                  <div class="media-body">
                    <p class="meta-title mr-3">Sorry</p>
                    <p class="message-text">No new messages</p>
                  </div>
                  @endif

                </div>
              </div>
            </a>
          </div>
        </div>
      </li>

      <li class="nav-item dropdown notification-dropdown">
        <a href="javascript:void(0);" class="nav-link dropdown-toggle" id="notificationDropdown" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
